{
  "name": "aws-serverless-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Serverless Typescript Boilerplate",
  "keywords": [
    "Serverless",
    "AWS",
    "Lambda",
    "Node.js",
    "Typescript"
  ],
  "main": "handler.js",
  "scripts": {
    "lint": "eslint . --ext js,ts --cache --fix",
    "prettier": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/serverless offline -s dev",
    "debug": "export SLS_DEBUG=* && node --debug ./node_modules/.bin/serverless offline -s dev"
  },
  "dependencies": {
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^10.12.18",
    "@types/serverless": "^1.72.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "prettier": "^2.1.2",
    "serverless": "^2.10.0",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "Roberto Chupin building on code from serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
